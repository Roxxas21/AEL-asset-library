  <romext>zip</romext>
  <rompath>/home/kodi/AEL-ROMs/con-coleco/</rompath>
  <path_assets>/home/kodi/AEL-assets/con-coleco/</path_assets>
</launcher>
</advanced_emulator_launcher_configuration>
