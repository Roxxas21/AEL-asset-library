<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<advanced_emulator_launcher_configuration>
<launcher>
  <category>Nintendo</category>
  <name>Nintendo 64 DD (Retroarch)</name>
  <platform>Nintendo 64</platform>
  <application>/home/kodi/bin/retroarch</application>
  <args>-L /home/kodi/bin/libretro/mupen64plus_libretro.so -f -v &quot;$rom$&quot;</args>
  <romext>ndd</romext>
  <rompath>/home/kodi/AEL-ROMs/nintendo-n64dd/</rompath>
  <path_assets>/home/kodi/AEL-assets/nintendo-n64dd</path_assets>
</launcher>
</advanced_emulator_launcher_configuration>
